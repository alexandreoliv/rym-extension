{
	"content_scripts": [
		{
			"js": ["content_script.js"],
			"matches": ["https://*.rateyourmusic.com/*"],
			"run_at": "document_end",
			"all_frames": true
		}
	],
	"permissions": ["storage", "input"],
	"web_accessible_resources": [
		{
			"resources": ["rymExtension.js"],
			"matches": ["https://*.rateyourmusic.com/*"]
		}
	],
	"description": "This extension can modify webpages",
	"manifest_version": 3,
	"name": "RYM extension",
	"version": "1.0",
	"author": "Alexandre Oliveira"
}
